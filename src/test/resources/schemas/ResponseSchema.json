{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": [
        "string",
        "integer"
      ]
    },
    "Firstname": {
      "type": "string"
    },
    "Lastname": {
      "type": "string"
    },
    "Age": {
      "type": "integer"
    },
    "Emailid": {
      "type": "string",
      "format": "email"
    },
    "Address": {
      "type": "object",
      "properties": {
        "City": {
          "type": "string"
        },
        "District": {
          "type": "string"
        },
        "LandMark": {
          "type": "string"
        }
      },
      "required": [
        "City",
        "District",
        "LandMark"
      ]
    },
    "Skills": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "Name": {
            "type": "string"
          },
          "Level": {
            "type": "string"
          },
          "Years ofExperience": {
            "type": "integer"
          }
        },
        "required": [
          "Name",
          "Level",
          "Years ofExperience"
        ]
      }
    }
  },
  "required": [
    "id",
    "Firstname",
    "Lastname",
    "Age",
    "Emailid",
    "Address",
    "Skills"
  ],
  "additionalProperties": false
}
